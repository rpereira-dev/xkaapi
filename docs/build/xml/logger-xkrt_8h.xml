        <param><defname>X</defname></param>
        <initializer>    do {                                                                                \
        xkrt_error_t r = X;                                                             \
        if (r != XKRT_SUCCESS)                                                          \
            LOGGER_FATAL(&quot;`%s` failed with err=%s (%d)&quot;, #X, xkrt_error_to_str(r), r);  \
    } while (0)</initializer>
